<?xml version="1.0" encoding="utf-8"?>
<network-security-config>

	<base-config cleartextTrafficPermitted="true">
		<trust-anchors>
			<certificates
					overridePins="true"
					src="system" />
		</trust-anchors>
	</base-config>

	<domain-config cleartextTrafficPermitted="true">
		<domain includeSubdomains="true">pm25.com</domain>
	</domain-config>

</network-security-config>
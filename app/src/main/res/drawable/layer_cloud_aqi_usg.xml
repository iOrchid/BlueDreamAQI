<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="70dp"
                android:height="50dp" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <!--draw a rectangle with corner-->
    <item
        android:bottom="0dp"
        android:top="20dp">
        <shape android:shape="rectangle">
            <size
                android:width="70dp"
                android:height="30dp" />
            <solid android:color="@color/colorAQIUSG" />
            <corners android:radius="18dp" />
        </shape>
    </item>
    <!--draw a oval left-->
    <item
        android:bottom="15dp"
        android:left="10dp"
        android:right="30dp"
        android:top="3dp">
        <shape android:shape="oval">
            <size
                android:width="30dp"
                android:height="30dp" />
            <solid android:color="@color/colorAQIUSG" />
            <corners android:radius="15dp" />
        </shape>
    </item>
    <!--draw a oval right-->
    <item
        android:bottom="15dp"
        android:left="25dp"
        android:right="15dp"
        android:top="10dp">
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <solid android:color="@color/colorAQIUSG" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>
